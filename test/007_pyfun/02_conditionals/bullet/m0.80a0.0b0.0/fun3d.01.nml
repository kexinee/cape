&project
    project_rootname = 'arrow'
/

&raw_grid
    grid_format = 'aflr3'
    data_format = 'ascii'
/

&governing_equations
    eqn_type = 'compressible'
    viscous_terms = 'inviscid'
/

&reference_physical_properties
    mach_number = 0.8
    dim_input_type = 'nondimensional'
    angle_of_attack = 0.0
    angle_of_yaw = 0.0
    reynolds_number = 698345.1757942521
    temperature = 264.07222222222225
/

&inviscid_flux_method
    first_order_iterations = {'@expr': '[37, 12]'}
    flux_construction = 'roe'
    flux_construction_lhs = 'vanleer'
    flux_limiter = 'hvanalbada'
    freeze_limiter_iteration = 0
/

&nonlinear_solver_parameters
    schedule_iteration(1) = 1
    schedule_iteration(2) = 50
    schedule_cfl(1) = 100.0
    schedule_cfl(2) = 100.0
/

&linear_solver_parameters
    linear_projection = .true.
    meanflow_sweeps = 5
/

&code_run_control
    steps(1) = 100
    steps(2) = 100
    stopping_tolerance = 1e-20
    restart_read = 'on'
/

&global
    boundary_animation_freq = 100
/

&sampling_parameters
    number_of_geometries = 2
    sampling_frequency(1) = 200
    sampling_frequency(2) = 200
    type_of_geometry(1) = 'plane'
    type_of_geometry(2) = 'plane'
    label(1) = 'plane-y0'
    label(2) = 'plane-z0'
    plane_center(1,1) = 0.0
    plane_center(2,1) = 0.0
    plane_center(3,1) = 0.0
    plane_center(1,2) = 0.0
    plane_center(2,2) = 0.0
    plane_center(3,2) = 0.0
    plane_normal(1,1) = 0.0
    plane_normal(2,1) = 1.0
    plane_normal(3,1) = 0.0
    plane_normal(1,2) = 0.0
    plane_normal(2,2) = 0.0
    plane_normal(3,2) = 1.0
/

&sampling_output_variables
    mach = .true.
/

&boundary_output_variables
    number_of_boundaries = -1
    cp = .true.
    boundary_list = '7-9'
    ptot = .true.
/

&component_parameters
    number_of_components = 5
    component_name(1) = 'bullet_no_base'
    component_name(2) = 'bullet_total'
    component_name(3) = 'cap'
    component_name(4) = 'body'
    component_name(5) = 'base'
    component_input(1) = '7-8'
    component_input(2) = '7-9'
    component_input(3) = '7'
    component_input(4) = '8'
    component_input(5) = '9'
    component_sref(1) = 3.14159
    component_sref(2) = 3.14159
    component_sref(3) = 3.14159
    component_sref(4) = 3.14159
    component_sref(5) = 3.14159
    component_bref(1) = 2.0
    component_bref(2) = 2.0
    component_bref(3) = 2.0
    component_bref(4) = 2.0
    component_bref(5) = 2.0
    component_cref(1) = 2.0
    component_cref(2) = 2.0
    component_cref(3) = 2.0
    component_cref(4) = 2.0
    component_cref(5) = 2.0
    component_xmc(1) = 1.75
    component_xmc(2) = 0.0
    component_xmc(3) = 0.0
    component_xmc(4) = 1.75
    component_xmc(5) = 0.0
    component_ymc(1) = 0.0
    component_ymc(2) = 0.0
    component_ymc(3) = 0.0
    component_ymc(4) = 0.0
    component_ymc(5) = 0.0
    component_zmc(1) = 0.0
    component_zmc(2) = 0.0
    component_zmc(3) = 0.0
    component_zmc(4) = 0.0
    component_zmc(5) = 0.0
    component_count(1) = -1
    component_count(2) = -1
    component_count(3) = -1
    component_count(4) = -1
    component_count(5) = -1
/

&special_parameters
    large_angle_fix = 'on'
/

